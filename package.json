{
  "name": "prisma",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "express": "^4.18.2"
  },
  "scripts": {
    "format": "prettier --write  \"./**/*.{vue,js,ts,scss}\" && yarn prisma format",
    "migrate": "yarn prisma migrate dev --name init",
    "seed": "yarn ts-node ./prisma/seed.ts",
    "prestartserver": "node -e \"console.log('Running yarn tsc...')\" && yarn tsc",
    "startserver": "yarn prestartserver && node -e \"console.log('Building server...')\" && node ./build/index.js"
  }
}
